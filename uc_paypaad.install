<?php

/**
 * @file
 * Contains un/installation hook implementations.
 */

/**
 * Implements hook_enable().
 */
function uc_paypaad_enable() {
  drupal_set_message(st('Ubercart Paypaad Payment has been installed. Please set your settings in <a href="!link">configuration page</a>.',
    array('!link' => url('admin/store/settings/payment/edit/methods'))
  ));
}

